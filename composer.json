{
    "name": "waygou/nova-ux",
    "description": "UX visual components for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\NovaUx\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\NovaUx\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
